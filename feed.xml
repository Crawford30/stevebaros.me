---
layout: null
style: "http://milanaryal.com/assets/css/feed.css"
feed_limit: 25
sitemap: false
permalink: /feed.xml
---

<?xml version="1.0" encoding="utf-8"?>
{% if page.style %}<?xml-stylesheet type="text/css" href="{{ page.style }}"?>{% endif %}
<feed xmlns="http://www.w3.org/2005/Atom">
{% if site.url %}{% assign base = site.url %}{% elsif site.github.url %}{% assign base = site.github.url %}{% endif %}
  <title type="text">{{ site.title }}</title>
  <subtitle type="html">{{ site.description }}</subtitle>
  <link rel="self" type="application/atom+xml" href="{{ base }}{{ page.url }}"/>
  <link rel="alternate" type="text/html" href="{{ base }}/"/>
  <id>{{ base }}/</id>
  <icon>{{ base }}/favicon.ico</icon>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <author>
    <name>{{ site.author.name }}</name>
    <uri>{{ site.author.url }}</uri>
    <email>{{ site.author.email }}</email>
  </author>
  <rights>Copyright (c) {{ site.time | date: '%Y' }}, {{ site.author.name }}</rights>
  <generator version="{{ jekyll.version }}" uri="https://github.com/jekyll/jekyll">Jekyll</generator>
  {% for post in site.posts limit: page.feed_limit %}
  <entry>
    <title type="text">{{ post.title | xml_escape }}</title>
    <link rel="alternate" type="text/html" href="{{ base }}{{ post.url }}"/>
    <id>{{ base }}{{ post.id }}</id>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <author>
      <name>{% if post.author.name %}{{ post.author.name }}{% else %}{{ site.author.name }}{% endif %}</name>
      <uri>{% if post.author.url %}{{ post.author.url }}{% else %}{{ site.author.url }}{% endif %}</uri>
      <email>{% if post.author.email %}{{ post.author.email }}{% else %}{{ site.author.email }}{% endif %}</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
